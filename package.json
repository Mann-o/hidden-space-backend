{
  "name": "hidden-space-backend",
  "version": "1.0.0",
  "description": "CMS for HiddenSpace website",
  "main": "server.js",
  "scripts": {
    "dev": "adonis serve --dev",
    "dev:debug": "adonis serve --dev --debug",
    "bundle": "adonis bundle --production",
    "start": "cross-env NODE_ENV=production node server.js",
    "migrate:dev": "adonis migration:refresh && adonis seed",
    "migrate:production": "adonis migration:run --force"
