  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.1.0",
    "@adonisjs/bodyparser": "^2.0.9",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/drive": "^1.0.4",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.13",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/mail": "^3.0.10",
    "@adonisjs/redis": "^2.0.7",
    "@adonisjs/session": "^1.0.29",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/validator": "^5.0.6",
    "@elastic/elasticsearch": "^7.5.0",
    "adonis-acl": "^1.1.1",
    "adonis-bundler": "^1.0.1",
    "adonis-cache": "^0.3.4",
    "adonis-kraken": "^1.0.1",
    "aws-sdk": "^2.600.0",
    "axios": "^0.19.1",
    "date-fns": "^2.9.0",
    "inline-css ": "^2.5.1",
    "lodash": "^4.17.15",
    "pg": "^7.17.0",
    "uuid": "^3.3.3",
    "uuid-validate": "^0.0.3",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "faker": "^4.1.0",
    "stylus": "^0.54.7"
  },
  "autoload": {
    "App": "./app"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
